{
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "NEXT_TELEMETRY_DISABLED": "1",
      "SKIP_ENV_VALIDATION": "1",
      "NODE_ENV": "production",
      "VERCEL": "1"
    }
  },
  "buildCommand": "pnpm run build",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "outputDirectory": ".next"
}
